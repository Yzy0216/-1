  .swipr-box {
    width: 100%;
    height: 280rpx;
    padding: 20rpx 0rpx;
    background-color: #fff;
    box-shadow: 0rpx 2rpx 4rpx 0rpx rgba(0, 0, 0, 0.05);
  }
  .seiper-smbox {
    padding: 5rpx 30rpx;
    box-sizing: border-box;
  }
  
  .slide-image {
    display:flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
  }
  .ad-index {
    width: 100%;
    box-sizing: border-box;
    padding: 30rpx;
  }
  
  .ad-df {
    border-radius: 10rpx;
    overflow: hidden;
    box-shadow: 0rpx 2rpx 4rpx 0rpx rgba(0, 0, 0, 0.05);
  }
  .server-box {
   
    padding: 0rpx 30rpx 0rpx 30rpx;
    margin: 20rpx 0rpx;
    background-color: #fff;
    border-radius: 10rpx;
    padding-bottom: 15rpx;
    box-shadow: 0rpx 2rpx 4rpx 0rpx rgba(0, 0, 0, 0.05);
  }
  
  .server-t {
    text-align: center;
  }
  
  .server-name {
    padding: 15rpx 20rpx;
  }
  
  .server-text {
    border-bottom: 1rpx solid #f6f6f6;
    font-weight: bold;
    height: 40rpx;
    line-height: 40rpx;
    padding-bottom: 15rpx;
  }
  
  .list-server {
   box-sizing: border-box; 
  display: flex;
  /* justify-content:space-around; */
  flex-wrap: wrap;
  align-items: center;
  } 
  
  .listbox {
    box-sizing: border-box;
    width: 25%;
    text-align: center;
    padding: 15rpx 0rpx;
  } 
  
  .listimg {
    width: 100rpx;
    height: 100rpx;
  }
  
  .inm {
    margin-bottom: 60rpx;
  }
  
  .bottomLine {
    padding: 50rpx 0rpx;
    font-size: 24rpx;
    text-align: center;
    color: #8b8b8b;
  }